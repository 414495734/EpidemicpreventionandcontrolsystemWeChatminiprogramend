/* pages/login/index.wxss */
.content{
  background-size: 100% 100%;
  width: 100%;
  height: 100vh;
}
.textCard{
  padding-top: 30px;
  text-align: center;
  margin-bottom: 30px;
}
.textCard >text{
  font-size: 23px;
}
.cardOne{
  padding: 8px;
  border-radius: 7px;
  border: 1px solid #fff;
  box-shadow: 0 0 20rpx #333;
}
.cardViewOne{
  border-bottom: 1px solid #f7eded ;
}
.cardViewOne >text{
  font-size: 18px;
}
.cardViewTwo{
  width: 95%;
  padding-top: 20px;
}
.cardViewTwo >view{
  margin: 10px;
  border-radius: 5px;
  display: inline-block;
  width: 43%;
  height: 100px;
  line-height: 100px;
  text-align: center;
  background-color: hsl(180deg 74% 91%);
  margin-bottom: 30px;
  box-shadow: 0 0 20rpx #333;
}